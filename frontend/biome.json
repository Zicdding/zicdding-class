{
  "$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
  "extends": [
    "../biome.json"
  ],
  "linter": {
    "enabled": true,
    "rules": {
      "suspicious": {
        "noExplicitAny": "off"
      },
      "correctness": {
        "noUnusedVariables": "error",
        "noUnusedImports": "warn"
      }
    }
  }
}